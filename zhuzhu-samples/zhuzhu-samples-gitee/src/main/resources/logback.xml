<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d %p [%t] [%X{traceId}] %logger{5} - %m%n</pattern>
        </encoder>
    </appender>
    　　　
    <root level="DEBUG">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>
